<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorTrackTextView">
        <attr name="originalColor" format="color" />
        <attr name="changeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="QQStepView">
        <attr name="innerColor" format="color" />
        <attr name="outerColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="stepTextSize" format="dimension" />
        <attr name="stepTextColor" format="color" />

    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="innerBackground" format="color" />
        <attr name="outerBackground" format="color" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
        <attr name="roundWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="startNormal" format="reference" />
        <attr name="startFocus" format="reference" />
        <attr name="gradeNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="RatingStarView">
        <attr name="starDrawable" format="reference" />
        <attr name="numStars" format="integer" />
        <attr name="rating" format="float" />
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="menuRightMargin" format="dimension" />
    </declare-styleable>

</resources>